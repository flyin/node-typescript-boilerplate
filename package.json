{
  "name": "alexweb-api",
  "version": "0.0.1",
  "description": "alex-web.ru api server",
  "main": "server/app.js",
  "repository": {},
  "author": "Alexander Polesov",
  "license": "MIT",
  "scripts": {
    "start:dev": "concurrently \"npm run tsc:watch\" \"npm run start:dev-server\" \"npm run start:dev-worker\"",
    "start:dev-server": "just-wait -p server/app.js && nodemon --watch server server/app.js",
    "start:dev-worker": "just-wait -p server/worker.js && nodemon --watch server server/worker.js",
    "tsc:watch": "tsc -w"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "dotenv": "^4.0.0",
    "koa": "^2.0.0",
    "koa-compose": "^3.2.1",
    "koa-logger": "^2.0.0",
    "koa-router": "^7.1.0",
    "kue": "^0.11.5",
    "moment": "^2.17.1",
    "mongoose": "^4.7.8",
    "promise": "^7.1.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.0.37",
    "@types/dotenv": "^2.0.20",
    "@types/koa": "^2.0.37",
    "@types/koa-compose": "^3.2.0",
    "@types/koa-logger": "^2.0.2",
    "@types/koa-router": "^7.0.21",
    "@types/kue": "^0.9.31",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^4.7.3",
    "@types/node": "^7.0.4",
    "@types/promise": "^7.1.30",
    "@types/winston": "^2.2.0",
    "concurrently": "^3.1.0",
    "just-wait": "^1.0.9",
    "nodemon": "^1.11.0",
    "tslint": "^4.3.1",
    "tslint-config-standard": "^3.0.0",
    "typescript": "^2.1.5"
  }
}
